{
  "cards": [
    {
      "title": "Gulag",
      "width": 7,
      "settings": [
        {
          "key": "gulag_command",
          "label": "Command",
          "input_type": "string",
          "default": "gulag",
          "value": "gulag"
        },
        {
          "key": "gulag_severity",
          "label": "Severity",
          "help_tooltip": "The number of minutes a gulag will last for once passed",
          "input_type": "numeric",
          "default": 20,
          "value": 18,
          "properties": {
            "min": 0,
            "integer": true
          },
          "validation_steps": [
            "isNumber",
            "isInteger",
            { "key": "isInRange", "args": { "min": 0 } }
          ],
          "cli": {
            "name": "Gulag Severity",
            "emoji": "üî®",
            "message": "üî® This is the number of minutes a member will be confined to gulag. Half again per extra vote. Enter a number to modify it:",
            "input_mode": "replace"
          }
        },
        {
          "key": "gulag_threshold",
          "label": "Threshold",
          "input_type": "numeric",
          "default": 5,
          "value": 6,
          "properties": {
            "min": 0,
            "integer": true
          },
          "validation_steps": [
            "isNumber",
            "isInteger",
            { "key": "isInRange", "args": { "min": 0 } }
          ],
          "cli": {
            "name": "Gulag Threshold",
            "emoji": "‚öíÔ∏è",
            "message": "‚öíÔ∏è This is the number of reacts a gulag vote must get to be pass. Enter a number to modify it:",
            "input_mode": "replace"
          }
        },
        {
          "key": "gulag_emoji",
          "label": "Emoji",
          "help_tooltip": "The emoji used for gulagging. Use either a :shortcode: or an emoji ID",
          "input_type": "string_auto_complete",
          "default": "",
          "value": "",
          "properties": {
            "auto-complete-type": "emoji_shortcode",
            "append": ["emoji_display"]
          },
          "validation_steps": [
            {
              "key": "any",
              "of": [
                [
                  {
                    "key": "matchesRegex",
                    "regex": "^$"
                  }
                ],
                [
                  {
                    "key": "matchesRegex",
                    "regex": "(?:^:[A-Za-z0-9_~()-]{2,}:$)|(?:^[0-9]{7,20}$)"
                  },
                  "isEmoji"
                ]
              ]
            }
          ]
        },
        {
          "key": "gulag_role",
          "label": "Role",
          "help_tooltip": "The role added to gulagged users",
          "input_type": "string_auto_complete",
          "default": "",
          "value": "@kulak",
          "properties": {
            "auto-complete-type": "role-mention",
            "append": [
              {
                "key": "color_display",
                "properties": {
                  "color_source": "role_color"
                }
              }
            ]
          },
          "validation_steps": [
            {
              "key": "any",
              "of": [
                [
                  {
                    "key": "matchesRegex",
                    "regex": "^$"
                  }
                ],
                [
                  {
                    "key": "matchesRegex",
                    "regex": "(?:^@.+$)|^(?:^[0-9]{7,20}$)"
                  },
                  "isRole"
                ]
              ]
            }
          ]
        },
        {
          "key": "gulag_vote_format",
          "label": "Vote Message Format",
          "input_type": "string_highlighted",
          "default": "%{count} more %{emoji}'s to gulag %{user}",
          "value": "%{count} more %{emoji}'s to gulag %{user}",
          "properties": {
            "tokens": ["%{count}", "%{emoji}", "%{user}"]
          }
        },
        {
          "key": "gulag_duration_format",
          "label": "Remaining Duration Format",
          "input_type": "string_highlighted",
          "default": ":alarm_clock: %{duration}",
          "value": ":alarm_clock: %{duration}",
          "properties": {
            "tokens": ["%{duration}"]
          }
        }
      ]
    },
    {
      "title": "Miscellaneous",
      "width": 5,
      "settings": [
        {
          "key": "starboard_threshold",
          "label": "Starboard Threshold",
          "input_type": "numeric",
          "default": 5,
          "value": 7,
          "properties": {
            "min": 0,
            "integer": true
          },
          "validation_steps": [
            "isNumber",
            "isInteger",
            { "key": "isInRange", "args": { "min": 0 } }
          ],
          "cli": {
            "name": "Starboard Threshold",
            "emoji": "‚≠ê",
            "message": "‚≠ê This is the number of reacts a message must get to be starboarded. Enter a number to modify it:",
            "input_mode": "replace"
          }
        },
        {
          "key": "repost_deleted_messages",
          "label": "Repost Deleted Messages",
          "input_type": "switch",
          "default": true,
          "value": false,
          "validation_steps": ["isBoolean"],
          "cli": {
            "name": "Repost Deleted Messages",
            "emoji": "üóëÔ∏è",
            "message": "üóëÔ∏è If true, deleted messages will be reposted immediately. Enter true or false to modify it:",
            "input_mode": "replace"
          }
        },
        {
          "key": "bot_commands_channels",
          "label": "Bot Commands Channels",
          "input_type": "string_array_auto_complete",
          "default": [],
          "value": [],
          "properties": {
            "auto-complete-type": "channel-mention"
          },
          "validation_steps": [
            "isArray",
            {
              "key": "allPass",
              "steps": [
                {
                  "key": "matchesRegex",
                  "regex": "(?:^#.+$)|(?:^[0-9]{7,20}$)"
                },
                "isChannel"
              ]
            }
          ],
          "cli": {
            "name": "Bot Commands Channels",
            "emoji": "ü§ñ",
            "message": "ü§ñ Some verbose commands are limited to these channels. Mention channels to toggle them:",
            "input_mode": "array_toggle"
          }
        }
      ]
    },
    {
      "title": "Roles",
      "width": 5,
      "settings": [
        {
          "key": "default_role",
          "label": "Default Role",
          "input_type": "string_auto_complete",
          "default": "",
          "value": "",
          "properties": {
            "auto-complete-type": "role-mention",
            "append": [
              {
                "key": "color_display",
                "properties": {
                  "color_source": "role_color"
                }
              }
            ]
          },
          "validation_steps": [
            {
              "key": "any",
              "of": [
                [
                  {
                    "key": "matchesRegex",
                    "regex": "^$"
                  }
                ],
                [
                  {
                    "key": "matchesRegex",
                    "regex": "(?:^@.+$)|(?:^[0-9]{7,20}$)"
                  },
                  "isRole"
                ]
              ]
            }
          ],
          "cli": {
            "name": "Default Role",
            "emoji": "üõ°",
            "message": "üõ° New members will be automatically moved into this role. Enter a role id (`!roleids`) to change:"
          }
        },
        {
          "key": "joinable_roles",
          "label": "Joinable Roles",
          "input_type": "string_array_auto_complete",
          "default": [],
          "value": [],
          "properties": {
            "auto-complete-type": "role-mention",
            "extensions": [
              {
                "key": "color_display",
                "properties": {
                  "color_source": "role_color"
                }
              }
            ]
          },
          "validation_steps": [
            "isArray",
            {
              "key": "allPass",
              "steps": [
                {
                  "key": "matchesRegex",
                  "regex": "(?:^@.+$)|(?:^[0-9]{7,20}$)"
                },
                "isRole"
              ]
            }
          ],
          "cli": {
            "name": "Joinable Roles",
            "emoji": "‚öîÔ∏è",
            "message": "‚öîÔ∏è These are the roles that any member can join at will. Enter a list of role ids (`!roleids`) to toggle. Optionally enter a nickname for the role in the format `nickname::roleid` if the role's name is untypable:",
            "input_mode": "array_toggle"
          }
        }
      ]
    },
    {
      "title": "Modules",
      "width": 3,
      "settings": [
        {
          "key": "module_auto_responses",
          "label": "Auto Responses",
          "input_type": "switch",
          "default": true,
          "value": true,
          "validation_steps": ["isBoolean"]
        },
        {
          "key": "module_music",
          "label": "Music",
          "input_type": "switch",
          "default": true,
          "value": true,
          "validation_steps": ["isBoolean"]
        },
        {
          "key": "module_emoji_manager",
          "label": "Emoji Manager",
          "input_type": "switch",
          "default": true,
          "value": true,
          "validation_steps": ["isBoolean"]
        },
        {
          "key": "module_logs",
          "label": "Module Logs",
          "input_type": "switch",
          "default": true,
          "value": true,
          "validation_steps": ["isBoolean"]
        }
      ]
    },
    {
      "title": "Permissions",
      "width": 6,
      "settings": [
        {
          "key": "architus_admins",
          "label": "Architus Admins",
          "input_type": "string_array_auto_complete",
          "default": [],
          "value": [],
          "properties": {
            "auto-complete-type": "user-mention"
          },
          "validation_steps": [
            "isArray",
            {
              "key": "allPass",
              "steps": [
                {
                  "key": "matchesRegex",
                  "regex": "(?:^@.+$)|(?:^[0-9]{7,20}$)"
                },
                "isUser"
              ]
            }
          ],
          "cli": {
            "name": "Architus admins",
            "emoji": "üîê",
            "message": "üîê These members have access to more bot functions such as `!purge` and setting longer commands. Mention a member to toggle:",
            "input_mode": "array_toggle"
          }
        }
      ]
    }
  ]
}
