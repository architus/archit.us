{
  "name": "@architus/app",
  "private": true,
  "description": "Web dashboard and main website for the Architus project",
  "version": "1.1.0",
  "license": "MIT",
  "author": "architus",
  "scripts": {
    "start": "gatsby develop",
    "test": "jest --coverage",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "bash -c 'if [ \"$CI\" == \"true\" ]; then yarn run build && yarn run lint-base; else yarn run lint-base; fi'",
    "lint-base": "eslint --max-warnings 0 -f unix \"src/**/*.{js,jsx,ts,tsx,json}\" \"gatsby-*.{js,ts}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\" \"gatsby-*.{js,ts}\" && eslint --fix -f unix \"src/**/*.{js,jsx,ts,tsx,json}\" \"gatsby-*.{js,ts}\"",
    "typecheck": "bash -c 'if [ \"$CI\" == \"true\" ]; then yarn run build && yarn run typecheck-base; else yarn run typecheck-base; fi'",
    "typecheck-base": "tsc --noEmit",
    "clean": "rimraf coverage .cache .linaria-cache public src/__generated__"
  },
  "dependencies": {
    "@architus/facade": "^0.1.0",
    "@architus/lib": "^0.1.0",
