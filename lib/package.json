{
  "name": "@architus/lib",
  "private": false,
  "description": "Common library between frontend applications in the Architus project",
  "version": "0.1.0",
  "license": "MIT",
  "author": "architus",
  "scripts": {
    "test": "NODE_ICU_DATA=../node_modules/full-icu jest --coverage",
    "lint": "eslint --max-warnings 0 -f unix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\" && eslint --fix -f unix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf coverage"
  },
  "dependencies": {
    "@types/shallow-equals": "^1.0.0",
    "react": "^16.13.1",
    "shallow-equals": "^1.0.0"
  },
  "devDependencies": {
    "full-icu": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/architus/archit.us"
  },
  "bugs": {
    "url": "https://github.com/architus/archit.us/issues"
  }
}
